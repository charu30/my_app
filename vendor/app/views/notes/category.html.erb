<h1>Notes</h1>

<% @notes.each do |note| %>
 <p>
  <!--Display the notes in a list-->  
  <ul>
    <li>
	<%=h note.title %> 
  	<%=h note.description %>
	</li>
</ul>
</p>

<% end %>

 